{
  "name": "collaby",
  "main": "./lib/collaby",
  "version": "0.2.7",
  "description": "A rock solid collaboration package for the atom editor",
  "keywords": [
		"collaboration",
		"pair",
		"live"
  ],
	"activationCommands": {
		"atom-workspace": ["collaby:startServer","collaby:join"]
	},
  "repository": "https://github.com/Patcher56/collaby",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
	"consumedServices": {
    "status-bar": {
      "versions": {
        ">=1.0.0": "consumeStatusBar"
      }
    }
  },
  "dependencies": {
		"atom-space-pen-views": "^2.2.0",
    "socket.io": ">=2.0.3",
    "socket.io-client": ">=2.0.3",
    "emissary": ">=1.3.3",
    "event-kit": "^2.3.0",
    "underscore": "^1.8.3"
  }
}
